<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <LangVersion>latest</LangVersion>
    <NoWarn>CS1591;CS0649</NoWarn>
    <Version>0.7.0</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright $([System.DateTime]::UtcNow.ToString(yyyy)). All rights reserved</Copyright>
    <PackageTags>Aspose, ApprovalTests</PackageTags>
    <PackageIconUrl>https://raw.github.com/SimonCropp/Aspose.ApprovalTests/master/icon.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/SimonCropp/Aspose.ApprovalTests</PackageProjectUrl>
    <PackageOutputPath>..\..\nugets</PackageOutputPath>
    <Description>Extends ApprovalTests to allow approval via Aspose.</Description>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AssemblyOriginatorKeyFile>$(SolutionDir)Key.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
</Project>